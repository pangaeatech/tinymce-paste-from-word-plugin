{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es5",
    "module": "es2015",
    "lib": ["es2015", "dom"],
    "importHelpers": true,
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "types",
    "sourceMap": true,
    "composite": true,
    "outDir": "dist",
    "rootDir": ".",
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "strict": false,
    "noEmitOnError": true,
    "types": []
  }
}
